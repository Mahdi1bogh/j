                        <ol class="breadcrumb ms-auto">

                            {% if app.user %}
                                <div class="mb-3">
                                    <li>You are logged in as {{ app.user.emailUser }}, <a href="{{ path('app_logout') }}">Logout</a></li>
                                </div>
                            {% endif %}

                        </ol>

                    </div>
                </div>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="col-sm-12">
                    <div class="white-box">
                        {% block tableau %}

